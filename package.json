    "prom-client": "^15.1.3",
    "razorpay": "^2.9.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.26",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "zod": "^4.3.1"
