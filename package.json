{
  "name": "magic-pages-nest-project",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "NODE_ENV=development nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch -- --inspect=9229",
    "start:prod": "node dist/apps/magic-pages-nest-project/main",
    "start:otel": "NODE_ENV=development node --require ./otel-bootstrap.js dist/apps/magic-pages-api-gateway/main",
    "start:otel:prod": "NODE_ENV=production node --require ./otel-bootstrap.js dist/apps/magic-pages-api-gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=.e2e-spec.ts --testPathIgnorePatterns=.integration-spec.ts",
    "test:unit:watch": "jest --watch --testPathIgnorePatterns=.e2e-spec.ts --testPathIgnorePatterns=.integration-spec.ts",
    "test:integration": "NODE_ENV=test jest --config jest.integration.config.ts --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/e2e/jest-e2e.json --runInBand",
    "test:e2e:watch": "NODE_ENV=test jest --config ./test/e2e/jest-e2e.json --watch",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=.e2e-spec.ts --testPathIgnorePatterns=.integration-spec.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
