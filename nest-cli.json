{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/magic-pages-api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/magic-pages-api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/magic-pages-api-gateway",
  "projects": {
    "contract": {
      "type": "library",
      "root": "libs/contract",
      "entryFile": "index",
      "sourceRoot": "libs/contract/src",
      "compilerOptions": {
        "tsConfigPath": "libs/contract/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "global-config": {
      "type": "library",
      "root": "libs/global-config",
      "entryFile": "index",
      "sourceRoot": "libs/global-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/global-config/tsconfig.lib.json"
      }
    },
    "magic-pages-api-gateway": {
      "type": "application",
      "root": "apps/magic-pages-api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/magic-pages-api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/magic-pages-api-gateway/tsconfig.app.json"
      }
    }
  }
}
